generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String      @default(uuid()) @id
  login       String
  password    String
  version     Int         @default(1)
  createdAt   Float
  updatedAt   Float
}

model Track {
  id          String      @default(uuid()) @id
  name        String
  artistId    String?
  albumId     String?
  duration    Int
}

model Album {
  id          String      @default(uuid()) @id
  name        String
  year        Int
  artistId    String?
}
